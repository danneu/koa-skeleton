{
    "name": "koa-skeleton",
    "private": true,
    "description": "a simple, full-featured koa application",
    "version": "4.0.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/danneu/koa-skeleton"
    },
    "scripts": {
        "start": "node app.js",
        "start-dev": "nodemon --ext js,jsx,html app.js",
        "reset-db": "node ./sql/reset_db",
        "deploy": "git push heroku master",
        "pretty-fix": "prettier --write \"{src,sql,test}/**/*.js\"",
        "lint": "eslint src",
        "patch": "npm version patch",
        "minor": "npm version minor",
        "major": "npm version major",
        "postpublish": "git push origin master --follow-tags"
    },
    "prettier": {
        "semi": false,
        "singleQuote": true,
        "trailingComma": "es5",
        "tabWidth": 4
    },
    "engines": {
        "node": ">=7.x"
    },
    "dependencies": {
        "autolinker": "^1.4.3",
        "babel-preset-react": "^6.24.1",
        "babel-register": "^6.26.0",
        "bcrypt": "^1.0.3",
        "better-assert": "1.0.2",
        "debug": "^3.1.0",
        "dotenv": "^4.0.0",
        "escape-html": "^1.0.3",
        "interval-cache": "^1.0.0",
        "knex": "^0.14.0",
        "koa": "^2.2.0",
        "koa-better-static2": "^1.0.2",
        "koa-bodyparser": "^4.2.0",
        "koa-bouncer": "^6.0.0",
        "koa-compress": "^2.0.0",
        "koa-helmet": "^3.1.0",
        "koa-logger": "^3.1.0",
        "koa-router": "^7.1.1",
        "pg": "^7.3.0",
        "pg-extra": "^0.2.0",
        "prop-types": "^15.6.0",
        "react": "^16.1.1",
        "react-template-render": "^1.0.0",
        "recaptcha-validator": "^2.0.0",
        "timeago.js": "^3.0.2",
        "uuid": "^3.0.1"
    },
    "devDependencies": {
        "prettier": "^1.7.4"
    },
    "license": "MIT"
}
