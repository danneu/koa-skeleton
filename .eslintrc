{
    "extends": ["eslint:recommended", "react-app"],
    "parserOptions": {
        "ecmaVersion": 8,
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    },
    "plugins": ["react"],
    "env": {
        "es6": true,
        "node": true
    },
    "rules": {
        "no-console": ["error", { "allow": ["log", "warn", "error"] }]
    }
}
